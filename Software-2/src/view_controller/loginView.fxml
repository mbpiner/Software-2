<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="192.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view_controller.LoginController">
    <children>
        <Label fx:id="loginTitleLabel" layoutX="14.0" layoutY="30.0" prefHeight="18.0" prefWidth="250.0" text="Scheduler Database" textAlignment="CENTER">
            <font>
                <Font name="Arial Black" size="13.0" />
            </font>
        </Label>
        <Label fx:id="loginUsernameLabel" layoutX="14.0" layoutY="78.0" prefHeight="29.0" prefWidth="76.0" text="Username:">
            <font>
                <Font name="Arial Black" size="12.0" />
            </font>
        </Label>
        <Label fx:id="loginPasswordLabel" layoutX="14.0" layoutY="117.0" prefHeight="29.0" prefWidth="100.0" text="Password:">
            <font>
                <Font name="Arial Black" size="12.0" />
            </font>
        </Label>
        <TextField fx:id="loginUsernameField" layoutX="100.0" layoutY="82.0" />
        <TextField fx:id="loginPasswordField" layoutX="100.0" layoutY="121.0" />
        <Button fx:id="loginSubmitButton" layoutX="175.0" layoutY="156.0" mnemonicParsing="false" onAction="#loginSubmitButtonPushed" text="Login">
            <font>
                <Font name="Arial Black" size="12.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
